<%= form_for @contact do |f|%>
<%= f.label :company %>
<%= f.text_field :company %>

<%= f.label :contact_name %>
<%= f.text_field :contact_name %>

<%= f.label :location %>
<%= f.text_field :location %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :phone_number %>
<%= f.text_field :phone_number %>

<%= f.label :industry %>
<%= f.select :industry, ["Manufacturer", "Retailer", "Buyer"] %>

<%= f.submit %>

<% end %>