<% @user_contacts.each do |contact| %>
<%= link_to contact.contact_name, contact_path(contact) %>
<% end %>


<% if current_user.role == "Designer" %>

    <%= form_for @partnership do |f| %>

    <%= f.label :contact %>
    <%= f.collection_select :contact_id, Contact.all, :id, :company  %>

    <%= f.submit %>
    <% end %>

<% else %>
    <%= link_to "New Contact", new_contact_path %>
<% end %>
