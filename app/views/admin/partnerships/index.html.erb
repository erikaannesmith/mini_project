<div class = "wrapper">
<div class = "first-line">

<h1>Partnerships</h1>
<nav>
<%= link_to "Dashboard", admin_dashboard_index_path %>
<%= link_to "Contacts", admin_contacts_path %>
<%= link_to "Designers", admin_users_path %>
<%= link_to "Log Out", logout_path, method: :delete %>
</nav>
</div>
<ul>
<% @partnerships.each do |partnership| %>
    <li>
    <%= link_to "#{partnership.user.company_name}", admin_user_path(partnership.user) %> and <%= link_to "#{partnership.contact.company}", admin_contact_path(partnership.contact) %>    
    </li>
<% end %>
</ul>
</div>