<div class = "wrapper">
<div class = "first-line">
<h1>Edit</h1>
<nav>
<%= link_to "Dashboard", admin_dashboard_index_path %>
<%= link_to "Partnerships", admin_partnerships_path %>
<%= link_to "Contacts", admin_contacts_path %>
<%= link_to "Designers", admin_users_path %>
<%= link_to "Log Out", logout_path, method: :delete %>
</nav>
</div>
<div class = "content">
<%= form_for(([current_user, @contact]), url: admin_contact_path(@contact)) do |f| %>
    <%= f.label :contact_name %>
    <%= f.text_field :contact_name %></br></br>

    <%= f.label :company %>
    <%= f.text_field :company %></br></br>

    <%= f.label :location %>
    <%= f.text_field :location %></br></br>

    <%= f.label :email %>
    <%= f.text_field :email %></br></br>

    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %></br></br>

    <%= f.label :industry %>
    <%= f.select :industry, ["Manufacturer", "Retailer", "Buyer"] %></br></br>

    <%= f.submit %></br>

<% end %>    
</div>

</div>