<div class = "wrapper">
    <div class = "first-line">
        <h1>Contacts</h1>
        <nav>
        <%= link_to "Dashboard", admin_dashboard_index_path %>
        <%= link_to "Partnerships", admin_partnerships_path %>
        <%= link_to "Designers", admin_users_path %>
        <%= link_to "Log Out", logout_path, method: :delete %>
        </nav>
    </div>

<div class = "content" id = "contact-index">
<ul>
<% @contacts.each do |contact| %>
<li>
<%= link_to "#{contact.contact_name} @ #{contact.company}", admin_contact_path(contact) %>
<%= link_to "Edit", edit_admin_contact_path(contact) %>
<%= link_to "Delete", admin_contact_path(contact), method: "delete" %>
</li>
<% end %>

</ul>
<div class = "new-contact">
<%= link_to "New Contact", new_admin_contact_path %>
</div>
</div>
</div>