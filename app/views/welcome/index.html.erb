<div class = "dash">

<h1 id = "dash-index">DesignerDashboard</h1>

Log In:

<%= form_for(:session, url: login_path) do |f| %>
  <div class = "login">
  <%= f.label :email %>
  <%= f.text_field :email %><br/>

  <%= f.label :password %>
  <%= f.password_field :password %><br/>

  <%= f.submit "Log In" %>
  </div>
<% end %>

<%= link_to "Register", new_user_path %>
</div>
